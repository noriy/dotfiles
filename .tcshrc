if ( $?term ) then

set	notify
set	ignoreeof
set	promptchars	= "%#"
set	prompt		= "%{[01;32m%}%m%{[00m%}:%{[01;34m%}%c2%{[00m%}%# "
set	history		= 5000
set	savehist	= 5000 merge
set	savedirs	= 15
set	dextract
set	nobeep
set	pushdsilent
set	symlinks	= chase
set	path		= ($path /snap/bin /sbin /usr/sbin ~/bin)

if ( -e /usr/bin/vi ) then
	setenv	EDITOR	/usr/bin/vi
	setenv	VISUAL	/usr/bin/vi
else if ( -e /bin/vi ) then
	setenv	EDITOR	/bin/vi
	setenv	VISUAL	/bin/vi
endif
if ( -e /usr/bin/less ) then
	setenv	PAGER	/usr/bin/less
else
	setenv	PAGER	/usr/bin/more
endif

if ( ! -d ~/.terminfo ) then
	mkdir ~/.terminfo
endif

setenv	VTE_CJK_WIDTH	auto
setenv	LESS		qR
setenv	TERMINFO	~/.terminfo

alias	j	'jobs -l'
alias	h	'history 15'
alias	H	'history | less'
alias	d	'dirs -v'
alias	pd	pushd
alias	pp	popd
alias	d1	'pushd +1'
alias	d2	'pushd +2'
alias	d3	'pushd +3'
alias	d4	'pushd +4'
alias	d5	'pushd +5'
alias	d6	'pushd +6'
alias	d7	'pushd +7'
alias	d8	'pushd +8'
alias	d9	'pushd +9'
alias	more	less
alias	rm	'rm -i'
alias	cp	'cp -i'
alias	mv	'mv -i'
alias	ls	'ls --color=auto -aF'
alias	grep	'grep --color=auto'
alias	fgrep	'fgrep --color=auto'
alias	egrep	'egrep --color=auto'
alias	gdb	'gdb -q'
alias	bc	'bc -ql'
alias	dif	'diff -u =1'
alias	z	suspend
alias	cdd	'cd \!$; pushd; cd \!$; pushd'
alias	dif	'diff -u =1/\!$ \!$'
alias	lgrep	'grep --color=always \!* | less -R'
alias	lxcls	'lxc list -c ns4'

bindkey	'^w'	backward-delete-word
stty erase '^h' start undef stop undef
if ( `stty -a | grep -q discard` ) then
	stty discard undef
else
	stty flush undef
endif
endif
