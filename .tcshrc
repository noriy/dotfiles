if ( $?term ) then

set	notify
set	ignoreeof
set	promptchars	= "%#"
set	prompt		= "[%c2]%# "
set	history		= 1000
set	savehist	= 1000 merge
set	savedirs
set	dextract
set	path		= ($path /sbin /usr/sbin ~/bin)

setenv	EDITOR	/usr/bin/vi
setenv	GIT_AUTHER_NAME		"Noriyuki Shiota"
setenv	GIT_AUTHER_EMAIL	"noriyuki.shiota@miraclelinux.com"
setenv	VTE_CJK_WIDTH	auto

alias	j	'jobs -l'
alias	h	'history 15'
alias	H	'history | less'
alias	d	'dirs -v'
alias	pd	pushd
alias	pp	popd
alias	d1	'pushd +1'
alias	d2	'pushd +2'
alias	d3	'pushd +3'
alias	d4	'pushd +4'
alias	d5	'pushd +5'
alias	d6	'pushd +6'
alias	more	less
alias	rm	'rm -i'
alias	cp	'cp -i'
alias	mv	'mv -i'
alias	ls	'ls --color=auto -aF'
alias	grep	'grep --color=auto'
alias	fgrep	'fgrep --color=auto'
alias	egrep	'egrep --color=auto'
alias	gdb	'gdb -q'
alias	bc	'bc -ql'
alias	dif	'diff -u =1'
alias	z	suspend

bindkey	'^w'	backward-delete-word
stty erase '^h'

endif
