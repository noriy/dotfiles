if ( $?term ) then

set	notify
set	ignoreeof
set	promptchars	= "%#"
set	prompt		= "%{[01;32m%}%m%{[00m%}:%{[01;34m%}%c2%{[00m%}%# "
set	history		= 5000
set	savehist	= 5000 merge
set	savedirs
set	dextract
set	nobeep
set	path		= ($path /sbin /usr/sbin ~/bin)
set	lxchome		= ~/work/lxc/ubuntu14/home/nori

if ( -e /usr/bin/vi ) then
	setenv	EDITOR	/usr/bin/vi
	setenv	VISUAL	/usr/bin/vi
else if ( -e /bin/vi ) then
	setenv	EDITOR	/bin/vi
	setenv	VISUAL	/bin/vi
endif
if ( -e /usr/bin/less ) then
	setenv	PAGER	/usr/bin/less
else
	setenv	PAGER	/usr/bin/more
endif

setenv	GIT_AUTHER_NAME		"Noriyuki Shiota"
setenv	GIT_AUTHER_EMAIL	"noriyuki.shiota@miraclelinux.com"
setenv	VTE_CJK_WIDTH	auto
setenv	LESS	qR

alias	j	'jobs -l'
alias	h	'history 15'
alias	H	'history | less'
alias	d	'dirs -v'
alias	pd	pushd
alias	pp	popd
alias	d1	'pushd +1'
alias	d2	'pushd +2'
alias	d3	'pushd +3'
alias	d4	'pushd +4'
alias	d5	'pushd +5'
alias	d6	'pushd +6'
alias	d7	'pushd +7'
alias	d8	'pushd +8'
alias	d9	'pushd +9'
alias	more	less
alias	rm	'rm -i'
alias	cp	'cp -i'
alias	mv	'mv -i'
alias	ls	'ls --color=auto -aF'
alias	grep	'grep --color=auto'
alias	fgrep	'fgrep --color=auto'
alias	egrep	'egrep --color=auto'
alias	gdb	'gdb -q'
alias	bc	'bc -ql'
alias	dif	'diff -u =1'
alias	z	suspend
alias	cdd	'cd \!$; pushd >/dev/null; cd \!$; pushd >/dev/null'
alias	dif	'diff -u =1/\!$ \!$'
alias	lgrep	'grep --color=always \!* | less -R'

bindkey	'^w'	backward-delete-word
stty erase '^h' start undef stop undef

endif
